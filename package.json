{
	"name": "xplace",
	"version": "0.3.0",
	"description": "Switch key and value.",
	"main": "xplace.js",
	"support": "xplace.support.js",
	"deploy": "xplace.deploy.js",
	"browser": "xplace.deploy.js",
	"scripts": {
		"pristine": "rsetmod clean && flxc execute ./.install.sh",
		"clear": "rsetmod clean --clear",
		"build": "babel xplace.js --out-file xplace.support.js --source-maps && webpack --progress --display-modules",
		"pretest": "npm run clear && npm run build",
		"test": "node xplace-test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/volkovasystems/xplace.git"
	},
	"keywords": [
		"switch",
		"key",
		"value",
		"xplace"
	],
	"author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
	"contributors": [
		"John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>",
		"Vinse Vinalon <vinsevinalon@gmail.com>"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkovasystems/xplace/issues"
	},
	"homepage": "https://github.com/volkovasystems/xplace#readme",
	"dependencies": {
		"babel-runtime": "^6.25.0",
		"empt": "^0.27.0",
		"falzy": "^0.23.0"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-core": "^6.25.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-latest": "^6.24.1",
		"source-map-loader": "^0.2.1",
		"webpack": "^3.4.1"
	}
}
